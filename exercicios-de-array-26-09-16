//1
#include "stdio.h"
int main(void) {
    int elem[5];
    int i, dif;
    for(i=0; i<5; i=i+1){
    	scanf("%d", &[i]);
    		if(i==0){
    			dif = [i+1]-[i];
    			printf("i+1\n, i\n");
    		}
    		else{
    	       	dif = [i]-[i-1];
    	       	printf("i\n, i-1\n");
    		}
    }
    return 0;
}

//2
#include "stdio.h"
int main(void){
	float elem[n], media, dp;
    int i, n;
    printf("Digite o tamanho do array: \n");
    scanf("%d", &n);
    printf("Digite seus números: \n");
    scanf("%f", &elem);
    for(i=0; i<n; i=i+1){
    	media = ([0]+[i])/i;
    	dp = (([i]-media)+(1*n))/(n-1);
    	if([i]<media){
    		printf("Abaixo da média:[i]\n");
    	}
    	else{
    		printf("Acima da média:[i]\n";)
    	}
    	printf("dp"\n);
    }
}
